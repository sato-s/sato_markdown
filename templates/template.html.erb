<html>
  <head>
    <meta charset="UTF-8">
    <style>
      @font-face {
        font-family: 'default';
        src: url(data:font/truetype;charset=uft-8;base64,<%= font %>) format('truetype');
      }
      body {
        font-family: 'default';
      }
      <%= css %>
    </style>
    <style>
      .markdown-body {
        box-sizing: border-box;
        min-width: 200px;
        max-width: 980px;
        margin: 0 auto;
        margin-left: 300px;
        padding-left: 30px;
        padding-top: 20px;
      }

      @media (max-width: 767px) {
        .markdown-body {
          padding: 15px;
        }
      }
    </style>
  </head>
  <body class="language-none markdown-body">
    <%= toc %>
    <%= content %>
    <script type="text/javascript">
      <%= js %>
    </script>
  </body>
</html>
